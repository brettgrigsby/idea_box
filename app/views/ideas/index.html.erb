<h1>My Idea's</h1>

<p>
  <%= link_to "New Idea", new_idea_path %>
</p>

<% @ideas.each do |idea| %>
  <%= link_to idea.title, idea_path(idea) %> |
  <%= link_to "Edit", edit_idea_path(idea) %> |
  <%= link_to "Delete", idea, method: :delete %></br>
<% end %>

